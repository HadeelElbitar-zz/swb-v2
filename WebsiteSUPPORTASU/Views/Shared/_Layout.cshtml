@model WebsiteSUPPORTASU.Models.Subscribe
<!DOCTYPE html>
<html lang="en">
  <head>
      <title>@ViewBag.title</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta http-equiv="Content-type" content="text/html;charset=UTF-8" /> 

    <!-- Le styles -->
    <link href="@Url.Content("~/Content/assets/css/bootstrap.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/assets/css/bootstrap-responsive.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/assets/css/style.css")" rel="stylesheet">   
    <link href="@Url.Content("~/Content//assets/css/bounce.css")" rel="stylesheet">
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js")"></script>
    <![endif]-->

    <!-- Fav and touch icons -->
    <link rel="shortcut icon" href="@Url.Content("~/Content/assets/ico/favicon.ico")">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="@Url.Content("~/Content/assets/ico/apple-touch-icon-144-precomposed.png")">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="@Url.Content("~/Content/assets/ico/apple-touch-icon-114-precomposed.png")">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="@Url.Content("~/Content/assets/ico/apple-touch-icon-72-precomposed.png")">
    <link rel="apple-touch-icon-precomposed" href="@Url.Content("~/Content/assets/ico/apple-touch-icon-57-precomposed.png")">
  </head>

  <body  id="page-top">

    <!-- NAVBAR
    ================================================== -->
    <!-- Wrap the .navbar in .container to center it on the page and provide easy way to target it with .navbar-wrapper. -->
    <div class="container navbar-wrapper">

      <div class="navbar navbar-inverse">
        <div class="navbar-inner">
          <!-- Responsive Navbar Part 1: Button for triggering responsive navbar (not covered in tutorial). Include responsive CSS to utilize. -->
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="http://support-asu.org/"><img src="@Url.Content("~/Content/assets//img/support_logo.png")"></a>
          <!-- Responsive Navbar Part 2: Place all navbar contents you want collapsed withing .navbar-collapse.collapse. -->
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li><a href="@Url.Action("Index", "Home")">Home</a></li>
              <li><a href="@Url.Action("Index", "Events")">Events</a></li>
              <li class="disabled"><a href="#"><span style="color:#999999;">Gallary</span></a></li>
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Supportians <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><a href="@Url.Action("Index", "Supportians", new { Year = 2013 })">2013</a></li>
                  <li class="disabled"><a href="#">2012</a></li>
                  <li class="disabled"><a href="#">2011</a></li>
                  <li class="disabled"><a href="#">2010</a></li>
                  <li class="disabled"><a href="#">2009</a></li>
                </ul>
              </li>
              <li><a href="@Url.Action("Index", "Sponsors")">Sponsors</a></li>
              <li><a href="@Url.Action("Index", "AboutUs")">About us</a></li>
              <li><a href="#contact" data-toggle="modal">Contact us</a></li>
              <li class="disabled"><a href="#"><span style="color:#999999;">Registration</span></a></li>
            </ul>
          </div><!--/.nav-collapse -->
        </div><!-- /.navbar-inner -->
      </div><!-- /.navbar -->

    </div><!-- /.container -->


   <!-- Slider  -->
      @if(@ViewBag.Slider.Count != 0){
@RenderPage("~/Views/Shared/Slider.cshtml");
}

    @RenderBody()




      <!-- FOOTER -->
      <footer>

      <div id="footer">
        <div class="row footer-container">
          <div class="span3" id="fotrclm3">
            
          </div>

          <a href="#contact" data-toggle="modal"><div class="span2" id="feedback"></div></a>
            <form id="form" method="post">
                <div class="span3" id="subscribe">
                    <h4>Subscribe us now</h4>
                    <input class="input-block-level" type="text" placeholder="Your Name" name="Name" id="FullName" />
                    <input class="input-block-level" type="text" placeholder="Email address" name="Email" id="Email" />
                    <button class="btn btn-primary pull-right" type="submit">Subscribe</button>
                    <br />
                    <br />
                    <hr class="featurette-divider">
                </div>
            </form>
          <div class="span1 social-container">
            <ul>
              <a href="http://www.fb.me/SUPPORT.FCIS" target="blank"><li class="social" id="fb">Facebook</li></a>
              <a href="http://www.twitter.com/SupportASU" target="blank"><li class="social" id="twitter">Twitter</li></a>
            </ul>
          </div>

          <div class="span1 social-container">
            <ul>
              <a href="http://www.youtube.com/user/SUPPORTOrganization" target="blank"><li class="social" id="youtube">Youtube</li></a>
              <a href="https://plus.google.com/u/0/107186631445377259065" target="blank"><li class="social" id="google">Google+</li></a>
            </ul>
          </div>
      </div>
    </div>
        <p id="foot">© 2013 SUPPORT - All Rights Reserved</p>
      </footer>


      <a id="scroll-top" href="#page-top">
          <h1>^</h1>
      </a>


      <div id="contact" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
          <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
              <h3 id="myModalLabel">Contact Us</h3>
          </div>
          <div class="modal-body">
              <input type="text" class="input-block-level" placeholder="Full Name" id="ContactFullName">
              <input type="text" class="input-block-level" placeholder="Email address" id="ContactEmail">
              <input type="text" class="input-block-level" placeholder="Subject" id="Subject">
              <textarea class="input-block-level" placeholder="Message" id="Message"></textarea>
              <button class="btn btn-primary pull-right" type="submit" onclick="ContactUs()">Send</button>
          </div>
      </div>
  

    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="@Url.Content("~/Content/assets//js/jquery.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-transition.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-alert.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-modal.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-dropdown.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-scrollspy.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-tab.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-tooltip.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-popover.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-button.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-collapse.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-carousel.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/bootstrap-typeahead.js")"></script>
    <script src="@Url.Content("~/Content/assets//js/slide-top.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="@Url.Content("~/Content/comingsoon/js/custom.js")"></script>
    <script>
     function Subscribe() {

          var passData = {
              Email: document.getElementById('Email').value,
              FullName: document.getElementById('FullName').value
          };
          var url = '@Url.Action("AddSubscribe", "Subscribe")?name='+passData.FullName+"&email="+passData.Email;
          $.ajax(
              {url: url}).done(function (data) {
                  alert(data);
              });
      }

        function ContactUs() {
            var passData = {
                Email: document.getElementById('ContactEmail').value,
                FullName: document.getElementById('ContactFullName').value,
                Subject: document.getElementById('Subject').value,
                Message: document.getElementById('Message').value
            };
            var url = '@Url.Action("AddContact", "ContactUS")?FullName=' + passData.FullName + '&Email=' + passData.Email + '&Subject=' + passData.Subject + '&Message=' + passData.Message;
          $.ajax(
              { url: url }).done(function (data) {
                  alert(data);
              });
        }
        
    </script>
  
  </body>
</html>


